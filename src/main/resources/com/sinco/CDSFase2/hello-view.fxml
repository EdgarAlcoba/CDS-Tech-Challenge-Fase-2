    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <Label fx:id="welcomeText" prefHeight="794.0" prefWidth="1228.0" style="-fx-wrap-text: true;" VBox.vgrow="ALWAYS" />
    <Button onAction="#onHelloButtonClick" prefHeight="94.0" prefWidth="216.0" text="Hello!" />
